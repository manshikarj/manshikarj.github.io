{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  
  "title": {
    "text": "SECOND DOSE VACCINATED PEOPLE",

    "subtitle":"",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "black"
  },
  
   "data": {
    "url": "https://raw.githubusercontent.com/manshikarj/manshikarj.github.io/main/seconddose.csv",
    "format": {"type": "csv"}
  },
 
  
  "height": 300,
  "width": 310,
   
  "mark": {"type": "line", "point": false},
 
  "encoding": {

    "x": {
      "field": "date",
      "type": "temporal",
      "title":"months",
      "axis": {"grid": false}
    },

    "y": {
      "field": "cumPeopleVaccinatedSecondDoseByPublishDate",
      "type": "quantitative",
      "title": "vaccinated by second dose",
      "axis": {"grid": false}
    },

    "color": {
      "field": "areaName",
      "type": "nominal",
      "scale": {"scheme": "set2"},
      "title": "",
      "legend": {
        "orient": "top-left", 
        "fillColor": "#ffffff"}
    }
}
}